server:
  port: 9003
  
spring:
  application:
    name: api-gateway
    
  cloud:
    gateway:
      routes:
      - id: employee-service
        uri: lb://employee-service
        predicates:
        - Path=/employee/**
        
         - id: user-service
        uri: lb://user-service
        predicates:
        - Path=/user/**